package selfprogramming;
import java.util.Scanner;
public class Q4 
{

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		
		int a,b,c;
		int d = 0;
		
		System.out.print("Enter a number: ");
		a = in.nextInt();
		System.out.print("Enter a number: ");
		b = in.nextInt();
		System.out.print("Enter a number: ");
		c = in.nextInt();
		
		for (int j = 1; j < a; j++)
			{
			d = b + c;
			b = c;
			c = d;
			System.out.print(c + " ");
			}		
			
	}

}
